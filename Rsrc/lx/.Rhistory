y0 <- mean(par("usr")[3:4])
pos.text <- ifelse(x < y0, 3, 1)
}
text(mid, x, as.character(x), pos=pos.text, offset=off.text,
col=col.text, cex=cex.text, srt=srt.text)
invisible(mid)
}
