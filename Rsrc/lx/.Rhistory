res <- as.data.frame(res, stringsAsFactors=F)
res <- as.data.frame(lapply(res, unlist, recursive=F, use.names=F), stringsAsFactors=F)
if (SIMPLIFY && (ncol(res) == 1)) res <- res[,1]
res
}
